{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@2.15.10"
    },
    "api/ping.js": {
      "runtime": "@vercel/node@2.15.10"
    },
    "api/minimal.js": {
      "runtime": "@vercel/node@2.15.10"
    }
  },
  "routes": [
    {
      "src": "/ping",
      "dest": "/api/ping.js"
    },
    {
      "src": "/api(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/api/minimal.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
